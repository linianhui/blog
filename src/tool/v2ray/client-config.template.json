{
  "log": {
    "logLevel": "None"
  },
  "inbounds": [
    {
      "listen": "$V2RAY_CLIENT_IP",
      "port": 1080,
      "protocol": "socks",
      "settings": {
        "auth": "noauth",
        "udp": true
      }
    }
  ],
  "outbounds": [
    {
      "protocol": "vless",
      "settings": {
        "vnext": [
          {
            "address": "$V2RAY_SERVER_IP",
            "port": 443,
            "users": [
              {
                "id": "$TEMPLATE_CLIENT_ID",
                "encryption": "none"
              }
            ]
          }
        ]
      },
      "streamSettings": {
        "network": "tcp",
        "security": "tls",
        "tlsSettings": {
          "serverName": "$TEMPLATE_SERVER_DOMAIN_NAME",
          "allowInsecure": false,
          "allowInsecureCiphers": false,
          "disableSystemRoot": true,
          "certificates": [
            $TEMPLATE_CERTIFICATE_JSON
          ]
        }
      },
      "mux": {
        "enabled": false
      }
    },
    {
      "protocol": "freedom",
      "settings": {},
      "mux": {
        "enabled": false
      }
    }
  ]
}