{
  "log": {
    "logLevel": "warning"
  },
  "inbounds": [
    {
      "listen": "127.0.0.1",
      "port": 1080,
      "protocol": "socks",
      "settings": {
        "auth": "noauth",
        "udp": true
      }
    }
  ],
  "outbounds": [
    {
      "protocol": "vmess",
      "settings": {
        "vnext": [
          {
            "address": "$TEMPLATE_SERVER_IP",
            "port": 443,
            "users": [
              {
                "id": "$TEMPLATE_CLIENT_ID",
                "alterId": 16,
                "security": "auto"
              }
            ]
          }
        ]
      },
      "streamSettings": {
        "network": "tcp",
        "security": "tls",
        "tlsSettings": {
          "serverName": "$TEMPLATE_SERVER_DOMAIN_NAME",
          "allowInsecure": false,
          "allowInsecureCiphers": true,
          "disableSystemRoot": true,
          "certificates": [
            $TEMPLATE_CERTIFICATE_JSON
          ]
        }
      }
    }
  ]
}