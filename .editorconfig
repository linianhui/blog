# http://editorconfig.org

root = true

[*]
charset                  = utf-8
end_of_line              = lf
indent_size              = 2
indent_style             = space
insert_final_newline     = false
trim_trailing_whitespace = true

[*.md]
trim_trailing_whitespace = false

[*.{bat,cmd,ps1}]
end_of_line = crlf
indent_size = 4

[*.js]
indent_size = 4

[*.sql]
indent_size = 4

[*.reg]
charset               = utf-16le
end_of_line           = crlf
